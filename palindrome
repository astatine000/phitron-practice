#include<iostream>
using namespace std;
int main(){
    int n ,i,j;
    cin>>n;
    long long int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        
    }
    
    for(i=0,j=n-1;i<j;i++,j--)
    {
        if(a[i]!=a[j]){
            cout<<"No"<<endl;
            break;
        }
    }
    if(i>=j) cout<<"Yes"<<endl;

    return 0;
}
